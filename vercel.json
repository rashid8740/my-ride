{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/next",
        "config": {
          "buildCommand": "cd Frontend && ./vercel-build.sh"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      }
    ]
  }